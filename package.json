{
  "name": "verkefni-9",
  "version": "1.0.0",
  "description": "Verkefni 9 sækir gögn um jarðskjálfta frá USGS og birtir á korti með [Leaflet](https://leafletjs.com/).",
  "main": "index.js",
  "scripts": {
    "test": "eslint *.js ./src/index.js ./src/lib/map.js ./src/lib/earthquakes.js",
    "test-fix": "eslint --fix --ext ./src/index.js.",
    "build": "rollup -c",
    "rollup-watch": "rollup -c -w",
    "browser-sync": "browser-sync start --server --files index.html styles.css scripts.js",
    "babel": "babel src --out-dir dist",
    "babel-watch": "babel src --out-dir dist --watch",
    "dev": "concurrently npm:browser-sync npm:rollup-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbl8/Verkefni-9.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lbl8/Verkefni-9.git/issues"
  },
  "homepage": "https://github.com/lbl8/Verkefni-9.git#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "rollup": "^2.33.1"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "leaflet": "^1.7.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
